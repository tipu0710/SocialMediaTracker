<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Alert">

    <ImageView
        android:id="@+id/app_icon_service"
        android:layout_centerInParent="true"
        android:src="@mipmap/ic_launcher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/app_name_service_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/app_icon_service"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="AppName"/>

    <Button
        android:id="@+id/add_more_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="add more 10 min"
        android:layout_below="@id/app_name_service_tv"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"/>

</RelativeLayout>